    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {},
  "lint-staged": {
    "*.{ts,tsx}": [
      "biome format --write",
      "biome lint --write",
